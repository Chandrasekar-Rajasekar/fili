{
  "makers": [
    {
      "name": "aggregateAverageByHour",
      "class": "com.yahoo.bard.webservice.data.config.metric.makers.AggregationAverageMaker",
      "params": {
        "innerGrain": "HOUR"
      }
    },
    {
      "name": "arithmeticPlus",
      "class": "com.yahoo.bard.webservice.data.config.metric.makers.ArithmeticMaker",
      "params": {
        "function": "PLUS"
      }
    },
    {
      "name": "arithmeticMinus",
      "class": "com.yahoo.bard.webservice.data.config.metric.makers.ArithmeticMaker",
      "params": {
        "function": "MINUS"
      }
    },
    {
      "name": "cardinalByRow",
      "class": "com.yahoo.bard.webservice.data.config.metric.makers.CardinalityMaker",
      "params": {
        "byRow": "true"
      }
    },
    {
      "name": "constant",
      "class": "com.yahoo.bard.webservice.data.config.metric.makers.ConstantMaker"
    },
    {
      "name": "longSum",
      "class": "com.yahoo.bard.webservice.data.config.metric.makers.LongSumMaker"
    },
    {
      "name": "doubleSum",
      "class": "com.yahoo.bard.webservice.data.config.metric.makers.DoubleSumMaker"
    },
    {
      "name": "bigThetaSketch",
      "class": "com.yahoo.bard.webservice.data.config.metric.makers.ThetaSketchMaker",
      "params": {
        "sketchSize": "4096"
      }
    },
    {
      "name": "count",
      "class": "com.yahoo.bard.webservice.data.config.metric.makers.CountMaker"
    }
  ],
  "metrics": [
    {
      "apiName": "COUNT",
      "longName": "COUNT",
      "description": "Description for count",
      "maker": "count"
    },
    {
      "apiName": "ADDED",
      "longName": "ADDED",
      "description": "Description for added",
      "maker": "doubleSum",
      "dependencyMetricNames": [
        "ADDED"
      ]
    },
    {
      "apiName": "DELTA",
      "description": "Description for delta",
      "maker": "doubleSum",
      "dependencyMetricNames": [
        "DELTA"
      ]
    },
    {
      "apiName": "DELETED",
      "description": "Description for deleted",
      "maker": "doubleSum",
      "dependencyMetricNames": [
        "DELETED"
      ]
    },
    {
      "apiName": "averageAddedPerHour",
      "description": "Calculate average added records by hour.",
      "maker": "aggregateAverageByHour",
      "dependencyMetricNames": [
        "added"
      ]
    },
    {
      "apiName": "averageDeletedPerHour",
      "description": "Calculate average deleted records by hour.",
      "maker": "aggregateAverageByHour",
      "dependencyMetricNames": [
        "deleted"
      ]
    },
    {
      "apiName": "plusAvgAddedDeleted",
      "description": "Calculate average added plus average deleted per hour.",
      "maker": "arithmeticPlus",
      "dependencyMetricNames": [
        "averageAddedPerHour", "averageDeletedPerHour"
      ]
    },
    {
      "apiName": "MinusAddedDelta",
      "description": "Calculate added minus delta per hour.",
      "maker": "arithmeticMinus",
      "dependencyMetricNames": [
        "added", "delta"
      ]
    },
    {
      "apiName": "cardOnPage",
      "description": "Generate cardinal rows on added.",
      "maker": "cardinalByRow",
      "dependencyMetricNames": [
        "page"
      ]
    },
    {
      "apiName": "bigThetaSketch",
      "description": "Calculate theta sketch on delta.",
      "maker": "bigThetaSketch",
      "dependencyMetricNames": [
        "delta"
      ]
    }
  ]
}


